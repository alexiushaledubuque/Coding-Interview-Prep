// Merge 2 sorted arrays

var sortedMerge = function(A, B){
	var temp = [];

	// Sorting is not necessary to begin merge of arrays
	while (A.length && B.length > 0){
		if (A[0] < B[0]){
			temp.push(A.shift());
		} else {
			temp.push(B.shift());
		}
	}

	while (A.length > 0){
		temp.push(A.shift());
	}

	while (B.length > 0){
		temp.push(B.shift());
	}

	A = temp;

	return A;
};

console.log([1,3,5,7,9], [2,4,6,8,10]);
